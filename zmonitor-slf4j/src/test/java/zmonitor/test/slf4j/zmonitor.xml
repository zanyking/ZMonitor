<?xml version="1.0" encoding="UTF-8" ?>
<zmonitor>
	 

	
	
	<monitor-sequence-handler name="console-handler" 
		class="org.zmonitor.handler.SampleConsoleMonitorSequenceHandler"/>
	
	
	<slf4j-conf defaultLogLevel="error" eatOperator="false">
		<!-- 
			default operator prefix:
				PUSH: >>
				POP: <<
		 -->
		<push><![CDATA[->]]></push>
		<pop><![CDATA[<-]]></pop>
		
		<property name="zmonitor.test.slf4j">trace</property>
		<property name="zmonitor.test.slf4j.clz">trace</property>
		
		
	</slf4j-conf>
</zmonitor>
